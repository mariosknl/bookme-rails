<div id="<%= dom_id booking_type %>">
  <p>
    <strong>Name:</strong>
    <%= booking_type.name %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= booking_type.location %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= booking_type.description %>
  </p>

  <p>
    <strong>Color:</strong>
    <%= booking_type.color %>
  </p>

  <p>
    <strong>Duration:</strong>
    <%= booking_type.duration %>
  </p>

  <p>
    <strong>Payment required:</strong>
    <%= booking_type.payment_required %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= booking_type.price %>
